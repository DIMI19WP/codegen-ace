{
    "manifest_version": 2,

    "name": "codegen_ace",
    "description": "코드젠에 ACE 에디터를 적용합니다",
    "version": "1",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "js": ["dist/main.js"],
            "matches": ["http://codegen.dimigo.hs.kr/submitpage.php?*"],
            "css": ["content/custom.css"],
            "run_at": "document_end"
        }
    ],
    "permissions": ["tabs", "http://*/*", "https://*/*"],
    "web_accessible_resources": ["content/*"]
}
